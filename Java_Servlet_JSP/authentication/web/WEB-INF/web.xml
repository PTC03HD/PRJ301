<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>MyFilter</filter-name>
        <filter-class>Filter.MyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>MyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Admin1</servlet-name>
        <servlet-class>controllers.Admin.Admin1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Member1</servlet-name>
        <servlet-class>controllers.Member.Member1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>guest</servlet-name>
        <servlet-class>controllers.Guest.guest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controllers.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ad2</servlet-name>
        <servlet-class>controllers.Admin.Ad2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admin1</servlet-name>
        <url-pattern>/Admin/Admin1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Member1</servlet-name>
        <url-pattern>/Member/Member1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>guest</servlet-name>
        <url-pattern>/guest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ad2</servlet-name>
        <url-pattern>/Admin/Ad2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
